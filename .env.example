# ETL Pipeline Configuration
# Copy this file to .env and fill in the values

# Input/Output paths (can be relative or absolute)
INPUT_DIR=data/input
OUTPUT_DIR=data/output

# Unstructured.io settings
# Strategy options: "fast" (default), "hi_res" (requires additional dependencies), "ocr_only" (for images)
UNSTRUCTURED_STRATEGY=fast

# Processing settings
# Maximum workers for parallel processing (set to 0 for no parallelism)
MAX_WORKERS=0

# Embedding settings (for Phase 3)
# EMBEDDING_MODEL=all-MiniLM-L6-v2
# EMBEDDING_DIMENSION=384

# Logging
LOG_LEVEL=INFO
# LOG_FILE=logs/etl.log  # Uncomment to enable file logging

# Database settings (for future use)
# DB_TYPE=postgres  # or sqlite, vector, etc.
# DB_URI=postgresql://user:password@localhost:5432/db_name 